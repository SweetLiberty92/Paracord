{
  "$schema": "https://raw.githubusercontent.com/nickkadutskyi/jb-tauri-v2-schema/main/schema.json",
  "productName": "Paracord",
  "version": "0.1.0",
  "identifier": "com.paracord.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Paracord",
        "width": 1280,
        "height": 800,
        "minWidth": 940,
        "minHeight": 560,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
